    "# # 한글 깨짐 방지 ( VS code )\n",
    "from matplotlib import font_manager, rc\n",
    "font_path = \"C:/Windows/Fonts/gulim.ttc\"\n",
    "\n",
    "# 폰트 이름을 가져옵니다.\n",
    "fontprop = font_manager.FontProperties(fname=font_path)\n",
    "font_name = fontprop.get_name()\n",
    "\n",
    "# 폰트 설정을 변경합니다.\n",
    "rc('font', family=font_name)"
   ]
  }
